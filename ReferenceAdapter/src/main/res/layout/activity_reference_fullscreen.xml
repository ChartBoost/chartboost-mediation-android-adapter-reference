<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/background_dark"
    tools:context="com.chartboost.mediation.referenceadapter.com.chartboost.mediation.referenceadapter.sdk.ReferenceFullscreenActivity">

    <WebView
        android:id="@+id/reference_fullscreen_webview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@android:color/background_dark"
        android:visibility="gone" />

    <VideoView
        android:id="@+id/reference_fullscreen_videoview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone" />

</RelativeLayout>
